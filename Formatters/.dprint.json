{
	"$schema": "https://dprint.dev/schemas/v0.json",
	"projectType": "educational",
	"incremental": true,
	"lineWidth": 80,
	"useTabs": true,
	"newLineKind": "lf",
	"json": {
		"indentWidth": 2,
		"commentLine.forceSpaceAfterSlashes": true,
		"ignoreNodeCommentText": "dprint-ignore"
	},
	"markdown": {
		"indentWidth": 2,
		"useTabs": false
	},
	"includes": ["**/*.{ts,js,cjs,mjs,json,md}"],
	"excludes": ["**/node_modules", "**/*-lock.json", "**/*-lock.yaml"],
	"plugins": [
		"https://plugins.dprint.dev/typescript-0.44.2.wasm",
		"https://plugins.dprint.dev/json-0.10.2.wasm",
		"https://plugins.dprint.dev/markdown-0.7.1.wasm"
	]
}
